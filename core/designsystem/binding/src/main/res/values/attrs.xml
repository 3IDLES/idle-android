<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string|reference" />

    <declare-styleable name="CareSubtitleTopAppBar">
        <attr name="title" />
        <attr name="showBackIcon" format="boolean" />
        <attr name="onBackClick" format="string" />
    </declare-styleable>

    <declare-styleable name="CareRow">
        <attr name="title" />
        <attr name="showArrow" format="boolean" />
        <attr name="showSwitch" format="boolean" />
        <attr name="switchState" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CareCenterInfoCard">
        <attr name="centerName" format="string|reference" />
        <attr name="centerAddress" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="CareDialog">
        <attr name="title" />
        <attr name="leftButtonText" format="string" />
        <attr name="rightButtonText" format="string" />
        <attr name="onLeftButtonClick" format="string" />
        <attr name="onRightButtonClick" format="string" />
    </declare-styleable>
</resources>